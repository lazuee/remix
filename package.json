{
  "name": "website",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "cross-env NODE_ENV=production remix vite:build",
    "build:analyze": "cross-env NODE_ENV=production remix vite:build --mode analyze",
    "dev": "cross-env NODE_ENV=development remix vite:dev",
    "start": "cross-env NODE_ENV=production node ./build/server/serve.mjs",
    "format": "pnpm run format:prettier && npm run format:stylelint",
    "format:prettier": "prettier --check --write --ignore-unknown --ignore-path .gitignore --log-level silent \"./**/*.{[jt]s?(x),m[jt]s,json,md}\"",
    "format:stylelint": "stylelint --fix --ignore-path .gitignore --quiet \"./**/*.{css,scss}\""
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "5.1.1",
    "@fontsource/pretendard": "5.1.0",
    "@hono/node-server": "1.13.2",
    "@remix-run/node": "2.13.1",
    "@remix-run/react": "2.13.1",
    "@vercel/analytics": "1.3.1",
    "@vercel/speed-insights": "1.0.12",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "hono": "4.6.5",
    "is-ip": "5.0.1",
    "isbot": "5.1.17",
    "react": "19.0.0-rc-fb9a90fa48-20240614",
    "react-dom": "19.0.0-rc-fb9a90fa48-20240614",
    "remix-themes": "1.5.1",
    "remix-utils": "7.7.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.0",
    "@eslint/js": "9.12.0",
    "@ianvs/prettier-plugin-sort-imports": "4.3.1",
    "@remix-run/dev": "2.13.1",
    "@resolid/remix-plugins": "2.2.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.7.5",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "browserslist-to-esbuild": "2.1.1",
    "clsx": "2.1.1",
    "cross-env": "7.0.3",
    "eslint": "9.12.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "postcss": "8.4.47",
    "postcss-import": "16.1.0",
    "postcss-import-ext-glob": "2.1.1",
    "postcss-load-config": "6.0.1",
    "prettier": "3.3.3",
    "prettier-plugin-css-order": "2.1.2",
    "prettier-plugin-jsdoc": "1.3.0",
    "prettier-plugin-tailwindcss": "0.6.8",
    "sass": "1.79.5",
    "stylelint": "16.10.0",
    "stylelint-config-clean-order": "6.1.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "6.7.0",
    "tailwind-merge": "2.5.4",
    "tailwind-scrollbar-utilities": "0.2.0",
    "tailwindcss": "3.4.14",
    "terser": "5.35.0",
    "typescript": "5.6.3",
    "typescript-eslint": "8.9.0",
    "vite": "5.4.9",
    "vite-tsconfig-paths": "5.0.1"
  },
  "pnpm": {
    "overrides": {
      "@remix-run/dev": "npm:@remix-run/dev",
      "@remix-run/node": "npm:@remix-run/node",
      "@remix-run/react": "npm:@remix-run/react",
      "@types/react": "npm:@types/react",
      "@types/react-dom": "npm:@types/react-dom",
      "esbuild": "npm:esbuild",
      "react": "npm:react@rc",
      "react-dom": "npm:react-dom@rc",
      "terser": "npm:terser"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": [
    "defaults and > 0.3%"
  ]
}
